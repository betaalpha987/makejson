<mvc:View
	controllerName="make.entitySetTable.EntitySetTable"
	xmlns:mvc="sap.ui.core.mvc"
	xmlns:t="sap.ui.table"
	xmlns="sap.m">
			<t:Table id="esTable" selectionMode="None" rows="{/tableData}" columnMenuOpen="onColumnMenuOpen" columnSelect="onColumnSelect" >
				<t:toolbar>
					<Toolbar>
						<Title id="title" text="{/title}"></Title>
						<ToolbarSpacer/>
						<Button icon="sap-icon://download" tooltip="Enable / Disable Visibility Menu Entries" press="onDownloadPressed"/>
					</Toolbar>
				</t:toolbar>
			</t:Table>

</mvc:View>